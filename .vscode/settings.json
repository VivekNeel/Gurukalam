{
    "todo-tree.defaultHighlight": {
      "foreground": "green",
      "type": "none"
    },
    "todo-tree.customHighlight": {
      "TODO": {},
      "FIXME": {}
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.rulers": [
      80,
